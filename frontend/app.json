{
  "expo": {
    "name": "Fast Friends",
    "slug": "fast-friends",
    "owner": "fast-friends",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "biz.fastfriends",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "biz.fastfriends",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "biz.fastfriends"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "./plugins/withSetPlaceholders.js"
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow FastFriends to use Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "21616515-6293-4025-b6c8-56e8faabefb5"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/21616515-6293-4025-b6c8-56e8faabefb5"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
